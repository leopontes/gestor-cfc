	<queryString>
		<![CDATA[select pg.*
  from processo_pagamento_pacote ppp
  join processo p on ppp.processo_id = p.processo_id
  join aluno a on p.aluno_id = a.aluno_id
  join pagamento pg on ppp.pagamento_id = pg.pagamento_id
 where a.aluno_id = $P{aluno_id}
   and p.data_termino is null;]]>
	</queryString>
